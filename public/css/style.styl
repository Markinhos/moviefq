body
  padding: 50px;
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
a
  color: #00B7FF;

.movie-thumbnail
	width: 61px;
	height: 91px;

.btm-links
	padding-bottom: 20px;

#navigation{
	.user-control {
		margin-right: 15px;
	}
}

.fluid-wrapper {
    position: relative;
    padding-bottom: 56.25%; /* 16:9 */
    height: 0;
    
	iframe {
	    position: absolute;
	    top: 0;
	    left: 0;
	    width: 100%;
	    height: 100%;
	    border: none;
	}
}

#feed {
	.media{
		padding: 8px;
		.media-object {
			width: 32px;
		}
	}
}

.movie{
	.add-watched-control {
		margin-right: 10px;
	}
	.movie-tags {
		margin-bottom: 20px;
	}
}

.tag {
	margin-right: 10px;
	> i:after {
		content: "\00a0"
	}
}

.follow-user {
	width: 263px;
	float: left;
	margin-right: 15px;
	.avatar {
		float: left;
		margin-right: 10px;
		width: 64px;
	}
}